
public class Main {

	public static void main(String[] args) {
		
		//Definizione Gestore delle consegne
		Gestore gestoreDelleConsegne = new Gestore("BRT");
		
		Drone d1 = new Drone("Parrot");
		
		Furgone f1 = new Furgone("Francesco");
		
		Bici b1 = new Bici("Haibike");
		
		gestoreDelleConsegne.aggiungiVeicoli(d1, f1, b1);
		
		Consegna c1 = new Consegna(d1, "consegna");
		Consegna c2 = new Consegna(f1, "consegna");
		Consegna c3 = new Consegna(b1, "consegna");	
		
		

	}

}
